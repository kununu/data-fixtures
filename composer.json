{
    "name": "kununu/data-fixtures",
    "description": "Load data fixtures for Doctrine Connections and Cache Pools",
    "type": "library",
    "require": {
        "php": "^7.2",
        "psr/cache": "^1.0",
        "doctrine/dbal": "^2.9"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.1"
    },
    "autoload": {
        "psr-4": {
            "Kununu\\DataFixtures\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kununu\\DataFixtures\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    }
}
